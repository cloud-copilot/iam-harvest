{
  "name": "@cloud-copilot/iam-harvest",
  "version": "0.1.1",
  "description": "Harvest data about IAM and publish to packages",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "clean": "rm -rf dist",
    "download": "tsx src/download.ts",
    "format": "npx prettier --write src/",
