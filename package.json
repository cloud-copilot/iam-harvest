  "author": "David Kerber <dave@cloudcopilot.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloud-copilot/iam-harvest/issues"
  },
  "homepage": "https://github.com/cloud-copilot/iam-harvest#readme",
  "dependencies": {
    "cheerio": "^1.0.0",
    "domhandler": "^5.0.3",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@cloud-copilot/prettier-config": "^0.1.1",
    "@types/node": "^22.9.1",
    "typescript": "^5.6.3"
  },
  "prettier": "@cloud-copilot/prettier-config"
