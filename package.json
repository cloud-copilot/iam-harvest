    "parse": "tsx src/parse.ts",
    "sortServiceDefinitions": "tsx src/sortServiceDefinitions.ts",
    "sanityCheck": "tsx src/sanityCheck.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloud-copilot/iam-harvest.git"
  },
  "keywords": [
